<%if !@micropost.nil?%>
$("#like_form_<%= @micropost.id -%>").html("<%= escape_javascript(render :partial => "likes/unlike_form",:locals => {:like => @like,:micropost => @micropost}) %>");
$("#likes_count_<%= @micropost.id -%>").html("<%= escape_javascript(render :partial=>'likes/likes_count',:locals=>{:like=>@like,:micropost=>@micropost}) %>");


<%end%>
<%if !@photo.nil?%>
$("#like_form_for_photo_<%= @photo.id -%>").html("<%= escape_javascript(render :partial=>'likes/unlike_form_for_photo',:locals=>{:like=>@like,:photo=>@photo}) %>");
$("#likes_count_for_photo_<%= @photo.id -%>").html("<%= escape_javascript(render :partial=>'likes/likes_count_for_photo',:locals=>{:like=>@like,:photo=>@photo})%>");			
<%end%>			
			


			