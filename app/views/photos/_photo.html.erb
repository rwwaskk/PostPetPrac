<%if !@album.nil?%>
	<% if !(photo.image.url=="/images/original/missing.png")%>
		<div id="photos">
			<ul class="thumbnails">
				<li class="span2">
					<%=link_to image_tag(photo.image.url(:thumb)) ,album_photo_path(@album,photo)%>
				</li>
			</ul>
		</div>
	<%end%>
<%end%>

<%if !@event.nil?%>	
<% if !(photo.image.url=="/images/original/missing.png")%>
		<div id="photos">
			<ul class="thumbnails">
				<li class="span2">
					<%=link_to image_tag(photo.image.url(:thumb)) ,event_photo_path(@event,photo)%>
				</li>
			</ul>
		</div>
	<%end%>
<%end%>