<% provide(:title, @title) %>
<div class="row">
  <aside class="span4">
  
      <%= render 'shared/stats' %>
      <% if @users.any? %>
     
          <% @users.each do |user| %>
           <%=user.name%>
          <% end %>
        </div>
      <% end %>
    </section>
  </aside>
  <div class="span8">
    <h3><%= @title %></h3>
    <% if @users.any? %>
      <ul class="users">
        <%= render @users %>
      </ul>
    
    <% end %>
  </div>
</div>